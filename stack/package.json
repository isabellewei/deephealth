{
  "name": "embers-vn",
  "version": "0.0.1",
  "description": "An online visual story-telling experience, created with React, Node.js, and Konva.",
  "main": "src/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/malusgreen/embersvn"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node_modules/.bin/babel-node src/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server src/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "author": "Kevin Zheng & Justin Duong",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "canvas": "^1.6.2",
    "cross-env": "^3.1.4",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "http-server": "^0.9.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  },
  "browser": {
    "canvas": false,
    "jsdom": false
  }
}
